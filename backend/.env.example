# хост postgres, если с докером то postgres
APP_CONFIG__DATABASE__HOST=postgres
# порт postgres
APP_CONFIG__DATABASE__PORT=5432
# имя пользователя postgres
APP_CONFIG__DATABASE__USER=postgres
# пароль пользователя postgres
APP_CONFIG__DATABASE__PASSWORD=password
# имя базы данных (нужно ОСТАВИТЬ такую же!)
APP_CONFIG__DATABASE__NAME=fruits

# хост сервера для доступа с других устройств (кроме сервера) поменять на 0.0.0.0
APP_CONFIG__RUN__HOST=0.0.0.0
# порт сервера
APP_CONFIG__RUN__PORT=8000
# Количество воркеров (количество ядер + 1)
APP_CONFIG__RUN__WORKERS=1
# Уровень логирвания
APP_CONFIG__RUN__LOG_LEVEL=error

# Использовать кастомную документацию с CDN (ускоряет работу)
APP_CONFIG__API__CREATE_CUSTOM_DOC_URLS=True